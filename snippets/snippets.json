{
  "Button": {
    "prefix": "zaButton",
    "body": [
      "<Button theme=\"${1|default,primary,danger|}\" block>$2</Button>",
        ""
    ],
    "description": "按钮,多种主题和尺寸"
  },
  "Icon": {
    "prefix": "zaIcon",
    "body": [
      "<Icon theme=\"${1|default,primary,success,warning,danger|}\" type=\"$2\" size=\"$3\" />"
    ],
    "description": "Icon图标，支持iconfont和自定义svg图标"
  },
  "Calendar": {
    "prefix": "zaCalendar",
    "body": [
      "<Calendar",
      "\tvalue={$1}",
      "\tonChange={(value) => {",
      "\tconsole.log('onChange', value);",
      "\t}}",
      "/>"
    ],
    "description": "日历，支持自定义渲染"
  },
  "Checkbox": {
    "prefix": "zaCheckbox",
    "body": [
      "<Checkbox type=\"${1|button, cell|}\" value={$2}>$2</Checkbox>"
    ],
    "description": "复选框，支持按钮选项组"
  },
  "Checkbox with Group": {
    "prefix": "zaCheckboxGroup",
    "body": [
      "<Checkbox.Group value={$1} onChange={}>",
      "\t<Checkbox value=\"0\">$3</Checkbox>",
      "\t<Checkbox value=\"1\">$4</Checkbox>",
      "</Checkbox.Group>"
    ],
    "description": "按钮组"
  },
  "DatePicker": {
    "prefix": "zaDatePicker",
    "body": [
      "<DatePicker",
      "\tvisible={$1}",
      "\tvalue={$2}",
      "\tmode=\"${1|year, month, date, time, datetime}\"",
      "\tonOk={(value) => {",
      "\t\tconsole.log('date', value);",
      "\t}}",
      "/>"
    ],
    "description": "日期选择器，各种组合，支持设置步进间隔"
  },
  "FilePicker": {
    "prefix": "zaFilePicker",
    "body": [
      "<FilePicker className=\"file-picker-btn\" accept=\"image/*\" onChange={$1}>",
      "\t<Icon type=\"add\" size=\"lg\" />",
      "</FilePicker>"
    ],
    "description": "文件选择器，支持多选和设置相机"
  },
  "Input": {
    "prefix": "zaInput",
    "body": [
      "<Input",
      "\tclearable",
      "\ttype=\"${1|text、number、idcard、price、password、search|}\"",
      "\tvalue={$1}",
      "\tplaceholder=\"请输入\"",
      "/>"
    ],
    "description": "输入框，内置多种输入类型，并且自带内置键盘"
  },
  "Picker": {
    "prefix": "zaPicker",
    "body": [
      "<Picker",
      "\tvisible={$1}",
      "\tvalue={$2}",
      "\tdataSource={$3}",
      "\tonOk={(selected) => {",
      "\t\tconsole.log('onOk: ', selected);",
      "\t}}",
    "/>"
  ],
  "description": "选择器，可以自定义列数，方便定义确认操作"
  },
  "Radio": {
    "prefix": "zaRadio",
    "body": [
      "<Radio value=\"$1\">选项一</Radio>"
    ],
    "description": "Radio"
  },
  "Radio with Group": {
    "prefix": "zaRadio",
    "body": [
      "<Radio.Group type=\"${1|button, cell|}\" shape=\"${2|rect, radius, round|}\">",
      "\t<Radio value=\"0\">选项一</Radio>",
      "</Radio.Group>"
    ],
    "description": "Radio with Group"
  },
  "SearchBar": {
    "prefix": "zaSearchBar",
    "body": [
      "<SearchBar",
        "\tshowCancel",
        "shape=\"${1|rect, radius, round|}\"",
        "\tvalue={$12}",
        "\tplaceholder=\"搜索\"",
        "\tcancelText=\"取消\"",
        "\tonChange={$3}",
      "/>"
    ],
    "description": "搜索栏，支持各种样式及自动获取焦点"
  },
  "Slider": {
    "prefix": "zaSlider",
    "body": [
      "<Slider",
        "\tmin={1}",
        "\tmax={100}",
        "\tvalue={$1}",
        "\tonChange={$2}",
      "/>"
    ],
    "description": "滑动输入条，支持多段设置及步长"
  },
  "StackPicker": {
    "prefix": "zaStackPicker",
    "body": [
      "\t<StackPicker",
        "\tmaskClosable",
        "\tvisible={$1}",
        "\tdataSource={$2}",
        "\tvalue={$3}",
        "\ttitle=\"层叠选择器标题\"",
        "\tonChange={(value) => {",
        "\t\tconsole.log('onChange', value);",
        "\t}}",
        "\tonOk={(value) => {",
        "\t\tconsole.log('onOk', value);",
        "\t}}",
      "/>"
    ],
    "description": "层叠选择器,允许任意选择层数"
  },
  "Stepper": {
    "prefix": "zaStepper",
    "body": [
      "<Stepper",
        "\ttitle=\"设置默认值\"",
        "\tshape=\"${1|rect, radius, circle|}\"",
        "\ttype=\"${2|text、number、price、tel|}\"",
        "\tonChange={$3}",
        "\tvalue={$4}",
      "/>"
    ],
    "description": "步进器，数字输入，步长定制"
  },
  "Switch": {
    "prefix": "zaSwitch",
    "body": [
      "<Switch checked={$1} onChange={$2} />"
    ],
    "description": "开关"
  },
}
